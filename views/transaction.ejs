<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>transactions</title>
</head>
<body>
    <ol>

        <%transactions.forEach(function(trans){%>
          <li>
            <div class="container">
                soldby:<%=trans.soldby%>
                purchasedby:<%=trans.purchasedby%>
                value:<%=trans.value%>   
                timestamp:<%=trans.timestamp%> 
            </div>
        </li> 
            
        <%})%>
    </ol>
</body>
</html>